<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <FrameLayout android:id="@+id/frameResetpassword"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylewhite_A702">
            <LinearLayout android:id="@+id/linearColumnarrowleft"
                android:layout_gravity="top|center"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar android:gravity="start"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowarrowleft"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginBottom="@dimen/_84pxv"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_9pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft_black_900_16x9"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_black_900_16x9"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumntittle"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_21pxh"
                            android:layout_weight="0.97"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:gravity="center_horizontal"
                                android:id="@+id/txtTittle"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="Change your password here"
                                style="@style/txtMontserratromanbold20_1" tools:text="Change your password here"/>
                            <TextView android:id="@+id/txtSubTittle"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_27pxv"
                                android:layout_width="match_parent"
                                android:text="Select which contact details should we use to change your password"
                                style="@style/txtMontserratromanregular16" tools:text="Select which contact details should we use to change your password"/>
                        </LinearLayout>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <EditText
                    android:background="@drawable/rectangle_border_bluegray_400_radius_22"
                    android:ellipsize="end"
                    android:textSize="15sp"
                    android:hint="Old Password"
                    android:id="@+id/etOldPassword"
                    android:inputType="textPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.03"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_21pxh"
                    android:paddingStart="@dimen/_18pxh"
                    android:paddingTop="@dimen/_23pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/black_900_75"
                    style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_border_bluegray_400_radius_22"
                    android:drawablePadding="@dimen/_30pxh"
                    android:ellipsize="end"
                    android:textSize="15sp"
                    android:hint="@string/lbl_new_password"
                    android:drawableEnd="@drawable/img_eye"
                    android:id="@+id/etNewPasswordFo"
                    android:inputType="textPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.03"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_21pxh"
                    android:paddingStart="@dimen/_18pxh"
                    android:paddingTop="@dimen/_23pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/black_900_75"
                    style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_border_bluegray_400_radius_22"
                    android:drawableEnd="@drawable/img_eye"
                    android:drawablePadding="@dimen/_30pxh"
                    android:ellipsize="end"
                    android:textSize="15sp"
                    android:hint="Confirm password"
                    android:id="@+id/etConfirmPasswor"
                    android:inputType="textPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.03"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_21pxh"
                    android:paddingStart="@dimen/_18pxh"
                    android:paddingTop="@dimen/_23pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/black_900_75"
                    style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnNext"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="220dp"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_23pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_23pxv"
                    android:singleLine="true"
                    android:text="Next"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_1" tools:text="Next"/>
            </LinearLayout>
        </FrameLayout>
    </ScrollView>
</layout>
