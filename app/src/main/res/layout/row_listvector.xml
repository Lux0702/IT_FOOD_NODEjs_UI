<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>

    <LinearLayout
        android:id="@+id/linearContent"
        style="@style/groupStylecornerRadius_4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="5dp">

        <ImageView
            android:id="@+id/imageVector"
            android:layout_width="@dimen/_34pxh"
            android:layout_height="@dimen/_34pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_marginBottom="@dimen/_34pxv"
            android:scaleType="fitXY"
            android:src="@drawable/img_vector_34x34"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_vector_34x34" />

        <LinearLayout
            android:id="@+id/linearNumber"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_29pxv"
            android:layout_marginEnd="@dimen/_120pxh"
            android:layout_marginBottom="@dimen/_29pxv"
            android:layout_weight="0.81"
            android:gravity="start"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtViaPhone"
                style="@style/txtMontserratromanregular16_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="Via Email"
                tools:text="Via Email" />

            <LinearLayout
                android:id="@+id/linearRowsignal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_marginBottom="@dimen/_1pxv"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/etPhoneNumber"
                    style="@style/txtManroperegular16"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_weight="0.3"
                    android:ellipsize="end"
                    android:singleLine="true"
                    tools:hint="......gmail.com" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
