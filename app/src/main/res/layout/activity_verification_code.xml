<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <FrameLayout android:id="@+id/frameVerificationCo"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A702">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_13pxv"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_9pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft_black_900_16x9"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_black_900_16x9"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTittle"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_weight="0.96"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="Enter your OTP code"
                        style="@style/txtMontserratromanbold20_1" tools:text="Enter your OTP code"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <ImageView android:id="@+id/imageUndrawconfirme"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_227pxh"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_81pxv"
                android:layout_width="@dimen/_230pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_undrawconfirme"
                tools:ignore="ContentDescription" tools:src="@drawable/img_undrawconfirme"/>
            <TextView android:gravity="start"
                android:id="@+id/txtSubTitle"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_100pxv"
                android:layout_width="match_parent"
                android:textSize="15sp"
                android:text="Code send to your phone in 01:00"
                style="@style/txtMontserratromanmedium14_1" tools:text="Code send to your phone in 01:00"/>

            <LinearLayout
                android:id="@+id/linearCodeNumber"
                style="@style/groupStylewhite_A700cornerRadius"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_13pxv"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/edtCode1"
                    android:layout_width="44dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="25dp"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_marginBottom="@dimen/_36pxv"
                    android:layout_weight="0.18"
                    android:background="@drawable/rectangle_bg_white_a700_75_border_bluegray_400_75_radius_4"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_1"
                    android:textAlignment="center"
                    android:textSize="50dp"
                    tools:text="@string/lbl_1" />

                <EditText
                    android:id="@+id/edtCode2"
                    android:layout_width="36dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_marginBottom="@dimen/_36pxv"
                    android:layout_weight="0.29"
                    android:background="@drawable/rectangle_bg_white_a700_75_border_bluegray_400_75_radius_4"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="9"
                    android:textAlignment="center"
                    android:textSize="50dp"
                    tools:text="9" />

                <EditText
                    android:id="@+id/edtCode3"
                    android:layout_width="38dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_marginBottom="@dimen/_36pxv"
                    android:layout_weight="0.26"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="2"
                    android:textAlignment="center"
                    android:background="@drawable/rectangle_bg_white_a700_75_border_bluegray_400_75_radius_4"
                    android:textSize="50dp"
                    tools:text="2" />

                <EditText
                    android:id="@+id/edtCode4"
                    android:layout_width="38dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_marginEnd="@dimen/_37pxh"
                    android:layout_marginBottom="@dimen/_36pxv"
                    android:layout_weight="0.27"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="3"
                    android:textSize="50dp"
                    android:textAlignment="center"
                    android:background="@drawable/rectangle_bg_white_a700_75_border_bluegray_400_75_radius_4"
                    tools:text="3" />
            </LinearLayout>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnVerify"
                style="@style/btnSolidRounded_1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="@dimen/_15pxh"
                android:ellipsize="end"
                android:gravity="center"
                android:paddingStart="0dp"
                android:paddingTop="0dp"
                android:paddingEnd="0dp"
                android:paddingBottom="0dp"
                android:singleLine="true"
                android:text="Verify"
                android:textAllCaps="false"/>
        </LinearLayout>
    </FrameLayout>
</layout>
